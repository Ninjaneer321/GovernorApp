    "@elastic/elasticsearch": "^7.15.0",
    "add-dashes-to-uuid": "^1.0.0",
    "apache-arrow": "^6.0.1",
    "axios": "^0.23.0",
    "bootstrap": "^5.1.3",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.6.5",
    "csv-parser": "^3.0.0",
    "csv-stringify": "^6.0.4",
    "csvtojson": "^2.0.10",
    "es6-promise-pool": "^2.5.0",
    "express": "^4.17.1",
    "iconv": "^3.0.1",
    "mkdirp": "^1.0.4",
    "mongodb": "^4.1.3",
    "nodemon": "^2.0.14",
    "underscore": "^1.13.1",
    "uuid": "^8.3.2",
